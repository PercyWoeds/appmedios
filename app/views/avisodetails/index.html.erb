<p id="notice"><%= notice %></p>

<h1>Listing Avisodetails</h1>

<table>
  <thead>
    <tr>
      <th>Descrip</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @avisodetails.each do |avisodetail| %>
      <tr>
        <td><%= avisodetail.descrip %></td>
        <td><%= avisodetail.comments %></td>
        <td><%= link_to 'Show', avisodetail %></td>
        <td><%= link_to 'Edit', edit_avisodetail_path(avisodetail) %></td>
        <td><%= link_to 'Destroy', avisodetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Avisodetail', new_avisodetail_path %>
