<%= simple_form_for(@customer_contrato) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :customer_id %>
    <%= f.input :secu_org %>
    <%= f.input :medio_id %>
    <%= f.input :contrato_id %>
    <%= f.input :anio %>
    <%= f.input :referencia %>
    <%= f.input :moneda_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
