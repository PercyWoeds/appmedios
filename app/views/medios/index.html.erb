<p id="notice"><%= notice %></p>

<h1>Medios </h1>

<table>
  <thead>
    <tr>
      <th>Razon Social</th>
      <th>Grupo</th>
      <th>Estacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medios.each do |medio| %>
      <tr>
        <td><%= medio.descrip %></td>
        <td><%= medio.grupo %></td>
        <td><%= medio.estacion %></td>
        <td><%= link_to 'Ver', medio %></td>
        <td><%= link_to 'Editar', edit_medio_path(medio) %></td>
        <td><%= link_to 'Eliminar', medio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medio', new_medio_path %>
