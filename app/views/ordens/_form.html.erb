<%= simple_form_for(@orden) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :contrato_id %>
    <%= f.input :fecha %>
    <%= f.input :medio_id %>
    <%= f.input :marca_id %>
    <%= f.input :version_id %>
    <%= f.input :fecha1 %>
    <%= f.input :fecha2 %>
    <%= f.input :tiempo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
