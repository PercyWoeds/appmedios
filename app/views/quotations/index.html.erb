<p id="notice"><%= notice %></p>

<h1>Cotizaciones</h1>

<table>
  <thead>
    <tr>
      <th>Fecha1</th>
      <th>Code</th>
      <th>Customer</th>
      <th>Punto</th>
      <th>Carga</th>
      <th>Tipo unidad</th>
      <th>Importe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotations.each do |quotation| %>
      <tr>
        <td><%= quotation.fecha1 %></td>
        <td><%= quotation.code %></td>
        <td><%= quotation.customer_id %></td>
        <td><%= quotation.punto_id %></td>
        <td><%= quotation.carga %></td>
        <td><%= quotation.tipo_unidad %></td>
        <td><%= quotation.importe %></td>
        <td><%= link_to 'Ver', quotation %></td>
        <td><%= link_to 'Editar', edit_quotation_path(quotation) %></td>
        <td><%= link_to 'Eliminar', quotation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<div class="button">
<%= link_to 'Nueva Cotizacion', new_quotation_path %>
</div>
