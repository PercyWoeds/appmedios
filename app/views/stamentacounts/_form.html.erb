<%= simple_form_for(@stamentacount) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :bank_account_id %>
    <%= f.input :fecha1 %>
    <%= f.input :fecha2 %>
    <%= f.input :saldo_inicial %>
    <%= f.input :saldo_final %>
    <%= f.input :user_id %>
    <%= f.input :company_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
