<p id="notice"><%= notice %></p>

<h1>Listing Tipo Ordens</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Descrip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_ordens.each do |tipo_orden| %>
      <tr>
        <td><%= tipo_orden.code %></td>
        <td><%= tipo_orden.descrip %></td>
        <td><%= link_to 'Show', tipo_orden %></td>
        <td><%= link_to 'Edit', edit_tipo_orden_path(tipo_orden) %></td>
        <td><%= link_to 'Destroy', tipo_orden, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo orden', new_tipo_orden_path %>
