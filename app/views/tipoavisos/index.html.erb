<p id="notice"><%= notice %></p>

<h1>Listing Tipoavisos</h1>

<table>
  <thead>
    <tr>
      <th>Descrip</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipoavisos.each do |tipoaviso| %>
      <tr>
        <td><%= tipoaviso.descrip %></td>
        <td><%= tipoaviso.comments %></td>
        <td><%= link_to 'Show', tipoaviso %></td>
        <td><%= link_to 'Edit', edit_tipoaviso_path(tipoaviso) %></td>
        <td><%= link_to 'Destroy', tipoaviso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipoaviso', new_tipoaviso_path %>
